@font-face {
font-family: "{{fontName}}";
src: {{{src}}};
font-weight:normal;
font-style:normal;
}

[class^="{{baseSelector}}-"]:before,
[class*=" {{baseSelector}}-"]:before {
font-family:{{fontName}};
display:inline-block;
line-height:1;
font-weight:normal;
font-style:normal;
speak:none;
text-decoration:inherit;
text-transform:none;
text-rendering:auto;
-webkit-font-smoothing:antialiased;
-moz-osx-font-smoothing:grayscale;
}

{{#each codepoints}}
  .{{../classPrefix}}{{@key}}:before {
  content: "\\{{this}}";
  }
{{/each}}

